@model IEnumerable<MVC_Example.Models.Employee>

@using MVC_Example.Models

@{
    ViewBag.Title = "Employee List";
}

<h2>Employee List</h2>

<ul>
    @foreach(Employee employee in Model)
    {
        <li>
            @Html.ActionLink(employee.name,"Details",new { id=employee.Id})
        </li>
    }
</ul>
@Html.ActionLink("Back to List","Index","Department")
 

